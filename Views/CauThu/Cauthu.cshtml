@model  IEnumerable<doan.Models.CauThu>

@{
    ViewBag.Title = "Cauthu";
    Layout = "~/Views/Shared/_Nen.cshtml";
}


<h2 style="text-align:center">Cauthu</h2>
<div class="container-fluid">   
    <div class="row">
        @foreach(var item in Model)
                {
             <div class="col-sm-6 col-md-4 col-lg-3"
                  <div class="div-img-client">
                      <img src="@Url.Content(item.Images) " height="150" width="150"
                  </div>
                  <div class="div-des-client">
                      <p>@Html.DisplayFor(model => item.MaCT)</p>
                      <p>@Html.DisplayFor(model => item.TenCT)</p>
                      <p>@Html.DisplayFor(model => item.ViTri)</p>
                  </div>
        }
    </div>

</div>

